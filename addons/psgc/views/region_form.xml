<odoo>
    <record id="view_region_form" model="ir.ui.view">
        <field name="name">region.form</field>
        <field name="model">region</field>
        <field name="arch" type="xml">
            <form string="Region">
                <sheet>
                    <group>
                        <field name="code_correspondence"/>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="geo_level"/>
                        <field name="old_name"/>
                        <field name="income_classification"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
