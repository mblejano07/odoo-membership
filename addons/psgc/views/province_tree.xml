<odoo>
    <record id="view_province_tree" model="ir.ui.view">
        <field name="name">province.tree</field>
        <field name="model">province</field>
        <field name="arch" type="xml">
            <tree string="Provinces">
                <field name="name"/>
                <field name="region_id"/>
                <field name="geo_level"/>
                <field name="income_classification"/>
            </tree>
        </field>
    </record>
</odoo>
