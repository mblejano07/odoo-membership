<odoo>
    <record id="view_province_form" model="ir.ui.view">
        <field name="name">province.form</field>
        <field name="model">province</field>
        <field name="arch" type="xml">
            <form string="Province">
                <sheet>
                    <group>
                        <field name="code_correspondence"/>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="geo_level"/>
                        <field name="old_name"/>
                        <field name="income_classification"/>
                        <field name="region_code"/>
                        <field name="region_correspondence"/>
                        <field name="region_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
